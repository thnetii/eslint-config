  "keywords": [
    "eslint",
    "eslint-config"
  ],
  "workspaces": [
    "src/*"
  ],
  "scripts": {
    "lint": "npm-run-all --continue-on-error lint:*",
    "lint:tsc": "tsc -p ./jsconfig.json",
