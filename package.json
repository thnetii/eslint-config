    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8",
    "typescript": "^5.0"
  }
