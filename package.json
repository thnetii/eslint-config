    "url": "git+https://github.com/thnetii/eslint-config.git"
  },
  "author": {
    "name": "TH-NETII Rasch Solutions",
    "email": "fredrik.rasch@gmail.com",
    "url": "https://thnetii.td.org.uit.no"
